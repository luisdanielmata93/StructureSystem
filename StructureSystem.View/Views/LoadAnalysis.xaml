                                <TextBox Grid.Row="9" Grid.Column="11" Template="{StaticResource NumericTextBox}" IsEnabled="False"/>
                                <TextBox Grid.Row="9" Grid.Column="13" Template="{StaticResource NumericTextBox}" IsEnabled="False"/>
