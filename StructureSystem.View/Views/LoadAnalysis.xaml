                                <TextBox Grid.Row="11" Grid.Column="13" Template="{StaticResource NumericTextBox}" IsEnabled="False"/>
                                <TextBox Grid.Row="11" Grid.Column="15" Template="{StaticResource NumericTextBox}" Text="{Binding LosaMacizaEntrepisoP.CargaVivaInstantanea,UpdateSourceTrigger=LostFocus}" />

                                <zoom:ZoomBorder  Grid.Row="13" Grid.Column="1" Grid.ColumnSpan="17" ClipToBounds="True" Background="White" >
                                    <Image Height="150" Source="/Content/Images/losa_macisa_entrepiso.png" />
                                </zoom:ZoomBorder>

                            </Grid>
                        </Controls:MetroTabItem>
