<UserControl x:Class="StructureSystem.View.Views.LoadAnalysis"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StructureSystem.View.Views" 
             xmlns:Controls="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
             xmlns:Icons="http://metro.mahapps.com/winfx/xaml/iconpacks"
             xmlns:zoom="clr-namespace:StructureSystem.View.Behaviors.ZoomBorder"
             xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
             xmlns:Be="clr-namespace:StructureSystem.View.Behaviors"
             xmlns:Dialog="clr-namespace:MahApps.Metro.Controls.Dialogs;assembly=MahApps.Metro"
             Dialog:DialogParticipation.Register="{Binding}"
    mc:Ignorable="d" 
             >
    <Grid>
        <Grid.ColumnDefinitions >
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <!--<Border Grid.Row="1" Grid.Column="0" Background="White">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="50"/>
                    <RowDefinition Height="30" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Label Grid.Row="0" Content="Es necesario cargar un proyecto para continuar ..." FontFamily="Century Gothic" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" FontSize="30"/>
                <Image Grid.Row="2" Source="/Content/Images/error2.png" />
            </Grid>
        </Border>-->
        
        <Controls:MetroTabControl Margin="10 20 10 10" Grid.Row="1" Grid.Column="0"  >
            <Controls:MetroTabItem Header="Análisis de carga">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0" />
                        <ColumnDefinition Width="50"/>
                        <ColumnDefinition Width="60"/>
                        <ColumnDefinition Width="10"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="130"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="100"/>
                        <ColumnDefinition Width="90"/>
                        <ColumnDefinition Width="20"/>
                        <ColumnDefinition Width="90"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="30"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="30"/>
                        <RowDefinition Height="20"/>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="10"/>
                    </Grid.RowDefinitions>

                    <Label Grid.Row="1" Grid.Column="1" Content="Nivel" />
                    <ComboBox Grid.Row="1" Grid.Column="2" SelectedValue="{Binding Storey,Mode=TwoWay}" ItemsSource="{Binding StoreyList, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" />

                    <Label  Grid.Row="1" Grid.Column="4"  Content="Sistema de piso" />
                    <ComboBox Grid.Row="1" Grid.Column="5" SelectedItem="{Binding FlooringMaterial}" DisplayMemberPath="Name" ItemsSource="{Binding FlooringMaterials, Mode=TwoWay,UpdateSourceTrigger=PropertyChanged}" IsEnabled="{Binding IsStoreySelected, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  />
                    <CheckBox Grid.Row="1" Grid.Column="7"  Content="Entrepiso" IsChecked="{Binding IsEntrepiso ,Mode=TwoWay}" IsEnabled="{Binding IsStoreySelected, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>
                    <CheckBox Grid.Row="1" Grid.Column="8"  Content="Azotea" IsChecked="{Binding IsAzotea, Mode=TwoWay}" IsEnabled="{Binding IsStoreySelected, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"/>

                    <TextBlock Grid.Row="1" Grid.Column="11" ToolTip="Nivel no guardado" VerticalAlignment="Center">
                        <TextBlock.Resources>
                            <Style TargetType="ToolTip" BasedOn="{StaticResource {x:Type ToolTip}}">
                                <Setter  Property="Background" Value="LightGoldenrodYellow"/>
                                <Setter Property="Foreground" Value="Black"/>
                                <Setter Property="FontWeight" Value="SemiBold"/>
                            </Style>
                        </TextBlock.Resources> 
                            <Icons:PackIconModern Kind="Alert" VerticalAlignment="Center" Visibility="{Binding IsAlert,Mode=TwoWay}"/>
                    </TextBlock>

                    <Button Grid.Row="1" Grid.Column="9" VerticalAlignment="Center" Command="{Binding SaveLevelCommand}" Content="Guardar" />

                    <!--<Image  Grid.Row="0" Grid.RowSpan="2" Grid.Column="11" Stretch="Uniform" VerticalAlignment="Bottom" HorizontalAlignment="Center" Source="/Content/Images/pisa.png" />-->

                    <Controls:MetroAnimatedTabControl Margin="0 -90 0 0" SelectedIndex="{Binding TabIndex, Mode=TwoWay}" Grid.Column="0" Grid.Row="3" Grid.ColumnSpan="13" >
                        <!--INICIO DE LOSA MACISA DE ENTREPISO-->
